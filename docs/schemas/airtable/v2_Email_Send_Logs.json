{
  "tableName": "v2_Email Send Logs",
  "metadata": {
    "base": "Last Mile Driver recruiting",
    "baseId": "app9N1YCJ3gdhExA0"
  },
  "fields": [
    {
      "name": "Template Key",
      "type": "Single Line Text",
      "description": "Identifier of the email template used",
      "mapping": "templateKey"
    },
    {
      "name": "Recipient Email",
      "type": "Single Line Text",
      "description": "Email address of the recipient",
      "mapping": "recipientEmail"
    },
    {
      "name": "Status",
      "type": "Single Select",
      "options": ["sent", "delivered", "failed", "bounced"],
      "description": "Current delivery status",
      "mapping": "status"
    },
    {
      "name": "Sent At",
      "type": "Date",
      "description": "Timestamp when email was sent",
      "mapping": "sentAt"
    },
    {
      "name": "Opened",
      "type": "Checkbox",
      "description": "Whether the email was opened",
      "mapping": "opened"
    },
    {
      "name": "Clicked",
      "type": "Checkbox",
      "description": "Whether any links were clicked",
      "mapping": "clicked"
    },
    {
      "name": "Bounced",
      "type": "Checkbox",
      "description": "Whether the email bounced",
      "mapping": "bounced"
    }
  ],
  "backendMapping": {
    "templateKey": "Template Key",
    "recipientEmail": "Recipient Email",
    "status": "Status",
    "sentAt": "Sent At",
    "opened": "Opened",
    "clicked": "Clicked",
    "bounced": "Bounced"
  }
}
