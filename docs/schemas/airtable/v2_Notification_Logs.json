{
  "tableName": "v2_Notification Logs",
  "metadata": {
    "base": "Last Mile Driver recruiting",
    "baseId": "app9N1YCJ3gdhExA0"
  },
  "fields": [
    {
      "name": "Rule ID",
      "type": "Single Line Text",
      "description": "ID of the notification rule triggered",
      "mapping": "ruleId"
    },
    {
      "name": "User ID",
      "type": "Single Line Text",
      "description": "ID of the user receiving notification",
      "mapping": "userId"
    },
    {
      "name": "Channel",
      "type": "Single Select",
      "options": ["email", "in_app", "push", "sms"],
      "description": "Delivery channel used",
      "mapping": "channel"
    },
    {
      "name": "Status",
      "type": "Single Select",
      "options": ["sent", "failed", "throttled"],
      "description": "Outcome of the dispatch",
      "mapping": "status"
    },
    {
      "name": "Timestamp",
      "type": "Date",
      "description": "Time of dispatch attempt",
      "mapping": "timestamp"
    },
    {
      "name": "Error",
      "type": "Long Text",
      "description": "Error details if status is failed",
      "mapping": "error"
    }
  ],
  "backendMapping": {
    "ruleId": "Rule ID",
    "userId": "User ID",
    "channel": "Channel",
    "status": "Status",
    "timestamp": "Timestamp",
    "error": "Error"
  }
}
