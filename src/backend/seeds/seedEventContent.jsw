/**
 * Seed seasonal events to Airtable
 */
export async function seedSeasonalEvents() {
    console.log('üåü Seeding seasonal events...');
    const tableName = getAirtableTableName('seasonalEvents');
    let created = 0;

    for (const event of SEASONAL_EVENTS) {
        try {
            const now = new Date().toISOString();
            await airtable.createRecord(tableName, {
                ...event,
                'Created At': now,
                'Updated At': now
            });
            created++;
            console.log(`  ‚úÖ Created event: ${event['Name']}`);
        } catch (error) {
            console.error(`  ‚ùå Failed to create event ${event['Name']}:`, error.message);
        }
    }

    console.log(`‚úÖ Seeded ${created}/${SEASONAL_EVENTS.length} seasonal events`);
    return { created, total: SEASONAL_EVENTS.length };
}

